{
  "name": "musicman-cli",
  "description": "",
  "main": "lib/cli.js",
  "scripts": {
    "dev": "nodemon --config ./nodemon.json lib/index.js",
    "start": "node lib/index.js",
    "daemon": "forever start lib/notify-daemon.js",
    "daemon:stop": "forever stop lib/notify-daemon.js",
    "lint": "esw lib test --color",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --watch --clear",
    "test": "NODE_ENV=test mocha",
    "test:watch": "NODE_ENV=test chokidar 'test/**/*.js' 'lib/**/*.js' -c 'node_modules/.bin/mocha'",
    "test:watch:unit": "chokidar 'test/**/*.js' -c 'node_modules/.bin/mocha \"{path}\"'",
    "test:coverage": "NODE_ENV=test nyc mocha"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "chai": "4.2.0",
    "chokidar": "3.3.1",
    "copy-dir": "1.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-watch": "6.0.1",
    "mocha": "7.0.1",
    "nodemon": "2.0.2",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "sinon": "8.1.1",
    "sinon-chai": "3.4.0"
  },
  "dependencies": {
    "chalk": "3.0.0",
    "columnify": "git+https://github.com/njhoffman/columnify.git#nick",
    "diff": "4.0.2",
    "flat": "5.0.0",
    "forever": "2.0.0",
    "glob": "7.1.6",
    "lodash": "4.17.15",
    "module-alias": "2.2.2",
    "node-id3": "0.1.13",
    "pretty-error": "2.1.1",
    "rc": "1.2.8",
    "term-size": "2.1.1"
  }
}
